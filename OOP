def__init__(self, IDP, MaxSpeedIDP, IncreaseAmountP):
    self.__ID= IDP
    self.__MaxSpeed= MaxSpeedIDP
    self.__CurrentSpeed= 0
    self.__HorizontalPosition= 0

IncreasedSpeed( MaxSpeed):
GetCurrentSpeed[[IncreasedSpeed-MaxSpeed]]:
